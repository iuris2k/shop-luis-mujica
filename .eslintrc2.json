{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"module.exports": {
		"extends": ["eslint:recommended", "plugin:prettier/recommended"]
	},
	"rules": {
		"prettier/prettier": "warn"
	}
}
